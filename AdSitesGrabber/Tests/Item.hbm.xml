<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="AdSitesGrabber.Tests" assembly="AdSitesGrabber">

  <!-- Mappings for class 'Item' -->
  <class name="Item" table="Items">

    <!-- Identity mapping -->
    <id name="Id">
      <column name="recId" />
      <generator class="guid.comb" />
    </id>

    <!-- Simple mappings -->
    <property name="Name" column="name" />

  </class>

</hibernate-mapping>